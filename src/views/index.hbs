<h1>Browser</h1>
<form action="" method="">
  <input type="text" class="search" name="search" placeholder="Search...">
  <input type="number" name="from" class="difficulty" placeholder="Difficulty from...">
  <span>-</span>
  <input type="number" name="to" class="difficulty" placeholder="Difficulty to...">
  <input type="submit" value="search">
</form>
{{#each cubes}}
<div class="cube">
  <img class="cube"
    src="{{cubes.imageUrl}}">
  <p class="name">{{cubes.name}}</p>
  <p><span>Difficulty level:</span> {{cubes.difficultyLevel}}</p>
  <a class="btn" href="/details/5c39f1851ab3b24f5c03e182">Details</a>
</div>
{{/each}}